    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "express-session": "^1.17.1",
    "graphql": "^14.7.0",
    "graphql-request": "^3.4.0",
    "graphql-tag": "^2.11.0",
    "http-errors": "~1.6.2",
    "lodash": "^4.17.21",
    "mongodb": "^3.6.5",
    "mongoose": "^5.12.2",
    "nodemon": "^2.0.7",
    "passport": "^0.4.1",
