{
  "name": "server",
  "version": "0.5.0",
  "engines": {
    "node": "^14.x",
    "npm": "^6.14.11"
  },
  "scripts": {
    "start": "node ./app.js",
    "server": "nodemon ./app.js",
    "client": "npm run start --prefix client",
